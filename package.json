{
  "name": "juni-cli-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*",
    "apps/*/client",
    "apps/*/server",
    "apps/*/renderer"
  ],
  "scripts": {
    "dev:web": "cd apps/web && npm run dev",
    "dev:proton": "cd apps/proton && npm run dev"
  }
}
